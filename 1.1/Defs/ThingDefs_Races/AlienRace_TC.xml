<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="SW_Droid_Base">
    <defName>TC_Protocol</defName>
    <label>TC Protocol Droid</label>
    <description>The TC-series protocol droid was a Human-cyborg relations protocol droid produced by Cybot Galactica.</description>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>0</maleGenderProbability>
        <forcedRaceTraitEntries>
          <li>
            <defName>ProtocolAI</defName>
            <chance>100</chance>
          </li>
        </forcedRaceTraitEntries>
        <notXenophobistTowards>
          <li>Human</li>
          <li>3PO_Protocol</li>
          <!--<li>TC_Protocol</li>-->
        </notXenophobistTowards>
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Female</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Female</li>
          </alienbodytypes>
          <useGenderedHeads>false</useGenderedHeads>
          <useGenderedBodies>false</useGenderedBodies>
          <customDrawSize>(1, 1)</customDrawSize>
          <customHeadDrawSize>(1.15, 1.15)</customHeadDrawSize>
          <customPortraitDrawSize>(1, 1)</customPortraitDrawSize>
          <customPortraitHeadDrawSize>(1.15, 1.15)</customPortraitHeadDrawSize>
          <headOffset>(0, 0)</headOffset>
          <bodyAddons>
            <li>
              <drawSize>1.15</drawSize>
              <path>Races/TC/Head/Female</path>
              <bodyPart>SW_Protocol_Head</bodyPart>
              <inFrontOfBody>true</inFrontOfBody>
              <useSkinColor>true</useSkinColor>
              <layerOffset>0.02</layerOffset>
              <offsets>
                <south>
                  <bodyTypes>
                    <Female>(-0.415, 0.2)</Female>
                  </bodyTypes>
                </south>
                <north>
                  <bodyTypes>
                    <Female>(0, 0.53)</Female>
                  </bodyTypes>
                </north>
                <east>
                  <bodyTypes>
                    <Female>(-0.43, 0.18)</Female>
                  </bodyTypes>
                  <crownTypes>
                    <Female>(0, 0)</Female>
                  </crownTypes>
                </east>
              </offsets>
            </li>
          </bodyAddons>
          <alienskincolorgen Class="ColorGenerator_Options">
            <options>
              <li>
                <!--Silver-->
                <weight>20</weight>
                <only>(200,200,200)</only>
              </li>
              <li>
                <!--Grey-->
                <weight>30</weight>
                <only>(120,120,120)</only>
              </li>
              <li>
                <!--Red-->
                <weight>10</weight>
                <only>(215,25,0)</only>
              </li>
            </options>
          </alienskincolorgen>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <body>Races/TC/</body>
          <head>Races/TC/Head/</head>
        </li>
      </graphicPaths>
    </alienRace>
